  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<typeAliases>
		<typeAlias type="com.toy.biz.user.UserVO" alias="user"></typeAlias>
		<typeAlias type="com.toy.biz.board.BoardVO" alias="board"/>
